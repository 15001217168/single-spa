{
  "name": "single-spa",
  "version": "2.0.6",
  "description": "Multiple applications, one page",
  "main": "lib/single-spa.js",
  "scripts": {
    "build": "npm run clean && babel src --out-dir lib",
    "prepublish": "npm run build",
    "clean": "rm -rf lib",
    "test": "jasmine",
    "build:canopy": "npm run build && rm -rf ../spalpatine/jspm_packages/npm/single-spa@2.0.5 && rsync -av . ../spalpatine/jspm_packages/npm/single-spa@2.0.5 --exclude node_modules --exclude .git --exclude --spec --exclude examples"
  },
  "repository": "https://github.com/CanopyTax/single-spa",
  "keywords": [
    "single",
    "page",
    "application",
    "spa",
    "multiple",
    "lifecycle",
    "systemjs",
    "jspm"
  ],
  "author": "Joel Denning",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "jasmine": "^2.4.1"
  },
  "ignore": [
    "examples"
  ]
}
